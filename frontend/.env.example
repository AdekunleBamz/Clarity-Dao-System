# Clarity DAO Frontend Environment Variables
# Copy this file to .env and fill in your values

# WalletConnect Project ID (required for wallet connection)
# Get yours at https://cloud.walletconnect.com
VITE_WALLETCONNECT_PROJECT_ID=

# Stacks Network (mainnet or testnet)
VITE_STACKS_NETWORK=mainnet

# Optional: Custom Stacks API endpoint
# VITE_STACKS_API_URL=https://api.hiro.so

# Optional: Enable debug logging
# VITE_DEBUG=true
